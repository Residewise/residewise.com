{% extends 'base.html.twig' %}

{% block title %}Set account{% endblock %}

{% block body %}
    <div class="container">
        <div class="row justify-content-center">
            {{ form_start(accountTypeForm) }}
            {{ form_row(accountTypeForm.account) }}
            <div>
                <button type="submit" class="btn btn-primary">
                    {{ 'save'|trans }}
                </button>
            </div>
            {{ form_end(accountTypeForm) }}
        </div>
    </div>
{% endblock %}


